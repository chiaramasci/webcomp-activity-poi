  "configuration": {
    "tagName": "activity-poi",
    "options": [
      {
        "key": "lat",
        "type": "text",
        "label": "Latitude of the center of the map",
        "required": false,
        "options": {
          "default": "46.4983"
        }
      },
      {
        "key": "lon",
        "type": "text",
        "label": "Longitude of the center of the map",
        "required": false,
        "options": {
          "default": "11.3548"
        }
      }
    ,
      {
        "key": "lan",
        "type": "select",
        "label": "Language",
        "required": false,
        "options": {
          "values": [
            "en",
            "de",
            "it"
          ],
          "default": "en"
        }
      },
      {
        "key": "categories",
        "type": "multiselect",
        "label": "Categories to show",
        "required": false,
        "options": {
          "values": [
            "All categories",
            "Winter",
            "Wellness Entspannung",
            "Verkehr und Transport",
            "Sommer",
            "Mobilitaet",
            "Kultur und Sehenswuerdigkeiten",
            "Geschaefte und Dienstleister",
            "Essen Trinken",
            "Anderes"
          ],
          "default": "All categories"
        }
      }
    ,
      {
        "key": "directions",
        "type": "null",
        "label": "Activate 'Get directions' button ",
        "required": false,
        "options": {
          "default": true
        }
      }
    ]
  }
}